{
    "name": "trsdsign/trs-admin-dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Butcher",
            "email": "joel@trsdesign.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "TrsDesign\\Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrsDesign\\Admin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "TrsDesign\\Admin\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    }
}
